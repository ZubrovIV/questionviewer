<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="csvReader" class="org.lessons.dao.impl.CsvResourceReaderImpl"/>
  <bean id="questionParser" class="org.lessons.service.impl.QuestionParserImpl"/>
  <bean id="questionService" class="org.lessons.service.impl.QuestionServiceImpl">
    <constructor-arg ref="csvReader"/>
    <constructor-arg ref="questionParser"/>
    <constructor-arg value="questions.csv"/>
  </bean>

</beans>
